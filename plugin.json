{
	"preload": "preload.js",
	"logo": "logo.png",
	"features": [
		{
			"code": "letpub",
			"explain": "查看期刊分区号",
			"cmds": [
				"th",
				"lw",
				"letpub"
			]
		},
		{
			"code": "article_replace",
			"explain": "替换论文的换行",
			"cmds": [
				"replace",
				{
					"type": "regex",
					"label": "替换pdf换行",
					"match": "/.{20,}/i"
				},
				{
					"type": "window",
					"label": "pdf应用替换复制内容",
					"match": {
						"app": [
							"AcroRd32.exe",
							"SumatraPDF.exe"
						],
						"class": [
							"AcrobatSDIWindow"
						]
					}
				}
			]
		}
	]
}
