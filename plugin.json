{
	"preload": "preload.js",
	"logo": "logo.png",
	"features": [
		{
			"code": "letpub",
			"explain": "查看期刊分区号",
			"cmds": [
				"thesis",
				"letpub"
			]
		},
		{
			"code": "pdf_replace",
			"explain": "替换pdf的换行",
			"cmds": [
				{
					"type": "regex",
					"label": "替换pdf换行",
					"match": "/.{5,}\\r\\n.{5,}/i",
					"minLength": 20
				},
				{
					"type": "window",
					"label": "pdf应用替换复制内容",
					"match": {
						"app": [
							"AcroRd32.exe",
							"SumatraPDF.exe"
						],
						"class": [
							"AcrobatSDIWindow"
						]
					}
				}
			]
		}
	]
}
